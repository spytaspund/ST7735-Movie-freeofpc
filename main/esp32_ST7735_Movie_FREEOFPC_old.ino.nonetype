const int bmp_allArray_LEN = 1;
const unsigned char* bmp_allArray[1] = {
  bmp_sdallfailbanner
};

uint16_t picture[120][160];

void setup(void) {
  WiFi.mode(WIFI_OFF);

  if(!SD.begin(SD_CS, SPI, 20000000)){
    tft.drawBitmap(0, 0, bmp_sdallfailbanner, 160, 128, ST7735_WHITE);
    tft.setRotation(3);
  }
  Serial.begin(115200);

  tft.initR(INITR_BLACKTAB);
  tft.fillScreen(ST7735_BLACK);
  tft.printString(0, 0, "Inited", 0xFFFF, 0x0000, 5, 6);

  Serial.println();
  Serial.println("Enter \"list\", \"play\", \"pause\", or \"stop\" for control");
  Serial.println();

}

void loop() {
  static File file;
  serout.trim();
  seroutold = serout;
  if (serout != seroutold) {
    /*count = count + 1;
      if(count > 2){
      count = 0;
      }*/
    if (serout == "list") {
      File root = SD.open("/");
      file = root.openNextFile();
      while (file) {
        if (file.isDirectory()) {
          WebSerial.print("DIR: ");
          WebSerial.println(file.name());
        }
        else {
          WebSerial.print("FILE: ");
          WebSerial.print(file.name());
          WebSerial.print("  SIZE: ");
          WebSerial.println(file.size());
        }
        file = root.openNextFile();
      }
      WebSerial.println();
    }
    /*if (serout != "play" and serout != "list") {
      if (!play) {
        if (waitFile) {
          String filename = serout;
          filename.trim();
          file = SD.open(filename);
          tft.fillScreen(ST7735_BLACK);
          WebSerial.println(filename);
          play = true;
          waitFile = false;
        }
      }
      }
      if (serout == "play") {
      //seroutold = serout;
      WebSerial.println("play");
