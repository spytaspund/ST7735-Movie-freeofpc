      waitFile = true;
      }*/
    if (serout != "list") {
      String filename = serout;
      filename.trim();
      file = SD.open(filename);
      tft.fillScreen(ST7735_BLACK);
      WebSerial.println("Playing " + filename + "...");
    }
  }
  if (serout != "list") {
    while (file.available()) {
      file.read((uint8_t*)picture, 120 * 160 * 2);
      tft.setRotation(0);
      tft.fillImage(picture, 0, 4, 160, 120);
      if (!file.available()) {
        file.close();
        break;
      }
    }
  }

}
void recvMsg(uint8_t *data, size_t len) {
  serout = "";
  for (int i = 0; i < len; i++) {
    serout += char(data[i]);
  }
  WebSerial.print(">" + serout);
  WebSerial.println();
}
